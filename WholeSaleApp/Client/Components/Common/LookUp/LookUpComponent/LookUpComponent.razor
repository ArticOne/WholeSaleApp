@inherits MudComponentBase
@inject IDialogService DialogService
@typeparam T
<MudPaper Style="@("padding: 1rem 1rem 1rem 0; " + Style)">
    <div style="display: flex; justify-content: space-between; align-items: center;">
        <MudText Typo="Typo.h6">@LookUpComponentTitle</MudText>
        <MudIcon @onclick="OpenDialog" Variant="Variant.Filled" Color="Color.Primary" Icon="@Icons.Material.Filled.Outbound" style="align-self: flex-start;"></MudIcon>
    </div>
    <MudPaper Elevation="0">
        @ChildContent
    </MudPaper>
</MudPaper>




